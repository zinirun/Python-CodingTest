    "def solution(n, m):\n",
    "    return [gcd(n,m), lcm(n,m)]\n",
    "\n",
    "def gcd(a,b):\n",
    "    while(b!=0): a, b = b, a%b\n",
    "    return abs(a)\n",
    "\n",
    "def lcm(a,b):\n",
    "    v = gcd(a,b)\n",
    "    if(v==0): return 0\n",
    "    return abs((a*b)/v)"
   ]
  }
