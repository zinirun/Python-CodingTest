    "def solution(n):\n",
    "    s = [False, False] + [True]*(n+1)\n",
    "    p = []\n",
    "    for i in range(2, n+1):\n",
    "        if s[i]:\n",
    "            p.append(i)\n",
    "            for j in range(2*i, n+1, i): s[j] = False\n",
    "    return len(p)"
   ]
