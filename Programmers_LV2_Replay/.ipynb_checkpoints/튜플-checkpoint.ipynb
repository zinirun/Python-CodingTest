    "def solution(s):\n",
    "    r=[]\n",
    "    s=sorted(s.replace('{','').replace('}}','').split('},'), key=len)\n",
    "    for i in s:\n",
    "        for j in list(map(int,i.split(','))):\n",
    "            if j not in r:\n",
    "                r.append(j)\n",
    "    return r"
   ]
  }
