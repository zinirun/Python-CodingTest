    "def solution(s):\n",
    "    cnt, zero = 0, 0\n",
    "    while True:\n",
    "        if s == \"1\": return [cnt, zero]\n",
    "        cnt += 1\n",
    "        zero += (len(s) - s.count('1'))\n",
    "        s = bin(s.count('1'))[2:]"
   ]
  }
