    "def solution(brown, yellow):\n",
    "    s = brown + yellow\n",
    "    for width in range(s, 2, -1):\n",
    "        if s % width == 0:\n",
    "            height = s // width\n",
    "            if (width - 2) * (height - 2) == yellow:\n",
    "                return [width, height]"
   ]
  }
