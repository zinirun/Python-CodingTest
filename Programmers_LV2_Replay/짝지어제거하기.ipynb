    "def solution(s): \n",
    "    stack = []\n",
    "    for i in s:\n",
    "        if len(stack) == 0: stack.append(i)\n",
    "        elif stack[-1] == i: stack.pop()\n",
    "        else: stack.append(i)\n",
    "    return 0 if stack else 1"
   ]
